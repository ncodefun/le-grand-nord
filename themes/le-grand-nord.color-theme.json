	"tokenColors": [
		{
			"settings": {
				"foreground": "#ffffff",
			}
		},
		{
			"name": "Base 'white'",
			"scope": [
				"variable",
				"meta.template.expression.js",
				"string.regexp.js",
				"string.regexp constant.other.character-class.set.regexp",
				"string.regexp.js constant.other.character-class.set.regexp constant.other.character-class.regexp",
				"source.python meta.fstring.python",
			],
			"settings": {
				"foreground": "#dddddd", // #969198
			}
		},	
		{
			"name": "COMMENT TAGS",
			"scope": [
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#4c6565BB"
			}
		},
		{
			"name": "COMMENT TEXT",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#4c6565"
			}
		},	
		{
			"name": "Storage (let, const, function, class); CSS pseudo ~ JUNIPER",
			"scope": [
				"storage",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
			],
			"settings": {
				"foreground": "#6e8e89"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"meta.brace",
				"punctuation",	
				"entity.other.attribute-name.id.css punctuation.definition.entity.css",
				"entity.other.attribute-name.class.css punctuation.definition.entity.css",
				"constant.character.format.placeholder.other.python",
			],
			"settings": {
				"foreground": "#6e8e89CC"
			}
		},
		{
			"name": "HTML elements, CSS ids and classes; JS punct. accessor ~ SEA NYMPH",
			"scope": [
				"entity.name.tag",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"punctuation.accessor",
				"punctuation.separator.key-value.js",
				"keyword.operator.type.annotation.ts",
			],
			"settings":{
				"foreground": "#7da9a5",
			}
		},
		{
			"name": "JS builtin ~ GULF STREAM",
			"scope": [
				"support.variable.dom",
				"support.constant",
				"variable.language",
				"support.class",
				"support.type.primitive.ts"
			],
			"settings": {
				"foreground": "#6db1a9"
			}
		},
		{
			"name": "Operators, Math signs ~ MONTE CARLO",
			"scope": [
				"keyword.operator",
				"keyword.control.anchor.regexp",
			],
			"settings": {
				"foreground": "#82cec5"
			}
		},
		{
			"name": "JS functions ~ MORNING GLORY",
			"scope": [
				"entity.name.function",
				"entity.name.function.tagged-template",
				"meta.function-call",
				"source.css support.function",
			],
			"settings": {
				"foreground": "#95dcd4",
			}
		},
		{
			"name": "Obj. keys, Func. params, Attr. names, Props ~ FRINGY FLOWER",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key string",
				"entity.other.attribute-name",
				"keyword.operator.quantifier.regexp",
				"support.type.property-name",
				"meta.function-call.arguments.pythonz",
				"support.variable.property",
			],
			"settings": {
				"foreground": "#b3e0d1",
			}
		},
		{
			"name": "JS 'new' ~ BAY LEAF",
			"scope": [
				"keyword.operator.new",
				"keyword.other.unit", // css units
				"entity.other.attribute-name.pseudo-element.css punctuation.definition.entity.css", // ::
			],
			"settings": {
				"foreground": "#83aa8a",
			}
		},		
		{
			"name": "JS class, @jsdocs ~ MADANG",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"entity.other.attribute-name.pseudo-element.css",
			],
			"settings": {
				"foreground": "#aadeb3"
			}
		},	
		{
			"name": "Values ~ FEIJOA",
			"scope": [
				"string",
				"constant",
				"source.css support.constant",
			],
			"settings": {
				"foreground": "#90D794",
			}
		},
		{
			"name": "JS String template; CSS vars, @media ~ TEA GREEN (PALE YELLOW-GREEN)",
			"scope": [
				"string.template",
				"string.regexp",
				"variable.css",
				"variable.scss",
				"constant.other.character-class.range.regexp",
				"punctuation.definition.keyword",
				"source.css keyword.control",
				"entity.other.keyframe-offset.percentage.css",
			],
			"settings": {
				"foreground": "#c6eab6", 
				"fontStyle": "italic"
			}
		},
		{
			"name": "JS string template -> faded quotes",
			"scope": [
				"string.template.js punctuation.definition.string.template ",
			],
			"settings": {
				"foreground": "#c6eab6AA",
				"fontStyle": ""
			}
		},
		{
			"name": "JS control flow – MEDIUM GRAY",
			// looks relatively redish beside other cool colors...
			"scope": [
				"keyword.control",
				"entity.name.label",
				"storage.modifier.async.js",
				"keyword.generator.asterisk.js",
			],
			"settings": {
				"foreground": "#7d8281",
			}
		},
		{
			"name": "CSS !important – MUTED PINK",
			"scope": [
				"keyword.other.important.css",
			],
			"settings": {
				"foreground": "#a290a5"
			}
		},	
		{
			"name": "JS debugger, delete, void, regex negation (^) – PALE PINK",
			"scope": [
				"keyword.other.debugger",
				"keyword.operator.negation",
				"keyword.operator.expression.delete",
				"keyword.operator.expression.void.js"
			],
			"settings": {
				"foreground": "#ceb0d3"
			}
		},
		{
			"name": "Strings, string templates, func. parameter, :pseudo-class – ITALIC",
			"scope": [
				"string",
				"string.template",
				"variable.parameter",
				"entity.other.attribute-name.pseudo-class.css",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "TAGGED string templates (lit-html) – NOT ITALIC",
			"scope": [
				"string.js.taggedTemplate",
			],
			"settings": {
				"fontStyle": ""
			}
		}
	]
}