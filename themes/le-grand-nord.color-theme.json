		"gitDecoration.stageModifiedResourceForeground": "#e2c08daa",
		"gitDecoration.submoduleResourceForeground": "#8db9e2",
		"gitDecoration.untrackedResourceForeground": "#73c991",
				
		"editorWidget.foreground": "#6E8E89",
		"icon.foreground": "#6E8E89",
		"statusBar.background": "#262b2b",
		"statusBar.debuggingBackground": "#2c5131",
		"tab.inactiveBackground": "#282e2e",
		"tab.activeBackground": "#293535",
		"tab.unfocusedHoverBackground": "#293535",
		"titleBar.activeForeground": "#cbfff9AA",
		
		"activityBar.foreground": "#6E8E89",
		"activityBar.inactiveForeground": "#6E8E89aa",
		"activityBar.activeBorder": "#6E8E89",
		"activityBarBadge.background": "#9cdc83",
		"activityBarBadge.foreground": "#000",
		"badge.background": "#9cdc83",
		"badge.foreground": "#000",
		"button.background": "#81a8a1",
		"dropdown.background": "#2a3232",
		"dropdown.foreground": "#cbfff9",
		"dropdown.listBackground": "#2a3232",
		"editor.foldBackground":"#ffffff03",
		"editor.findMatchBackground": "#a1bd00aa",
		"editor.findMatchHighlightBackground": "#babd0050",
		"editor.foreground": "#eeeeee",
		"editor.inactiveSelectionBackground": "#6867417e",
		"editor.lineHighlightBackground": "#86fff111",
		"editor.rangeHighlightBackground": "#ffffff11",
		"editor.selectionBackground": "#a3a11669",
		"editor.selectionHighlightBackground": "#ffffff22",
		"editor.wordHighlightBackground": "#ceff8022",
		"editor.wordHighlightStrongBackground": "#706f1344",
		"editorBracketMatch.background": "#4ab33c73",
		"editorBracketMatch.border": "#88888800",
		"editorHoverWidget.border": "#81a8a1",
		"editorIndentGuide.activeBackground1": "#cbffee88",
		"editorIndentGuide.background1": "#404040",
		"editorLineNumber.activeForeground": "#81a8a1",
		"editorLineNumber.foreground": "#81a8a160",
		"editorSuggestWidget.border": "#81a8a1",
		"editorSuggestWidget.selectedBackground": "#81a8a122",
		"editorWidget.background": "#24272a",
		"editorWidget.border": "#313738",
		"editorStickyScroll.background": "#1e2222",
		"editorStickyScrollHover.background": "#293535",
		"editorStickyScroll.shadow": "#00000017",
		"editorStickyScroll.border": "#2c3132",
		"focusBorder": "#bcfff74e",
		"input.background": "#1d2121",
		"input.foreground": "#ffffff",
		"input.placeholderForeground": "#90a9a5",
		"inputOption.activeBorder": "#90a9a5",
		"panel.border": "#81a8a160",
		"scrollbar.shadow": "#00000030",
		"sideBar.foreground": "#cbfff999",
		"sideBar.border": "#293333",
		"sideBarSectionHeader.foreground": "#81a8a1",
		"sideBarTitle.foreground": "#cbfff900",		
		"statusBar.foreground": "#cbffef68",
		"statusBar.noFolderForeground": "#cbfff999",
		"statusBar.noFolderBackground": "#222627",
		"terminal.ansiCyan": "#8dfef0",
		"terminal.ansiGreen": "#b0ff91",
		"terminal.ansiMagenta": "#ff5fb7",
		"terminal.ansiYellow": "#e7ff91",
		"terminal.foreground": "#ddd",
		"listFilterWidget.background": "#72ecd677",
		"listFilterWidget.outline": "#72ecd6",
		"listFilterWidget.noMatchesOutline": "#ec72c7",
		"list.activeSelectionBackground":"#81a8a144",
		"list.inactiveSelectionBackground": "#81a8a111",
		"list.inactiveFocusBackground": "#81a8a106",
		"list.focusBackground": "#81a8a133",
		"list.focusForeground": "#72ecd6",
		"list.hoverForeground": "#cbfff9",
		"list.dropBackground": "#383b3d",
		"editorWhitespace.foreground": "#262626",
		"minimap.background": "#24282a",
		"widget.shadow": "#00000040",
		"scrollbarSlider.background": "#5e797933",
		"scrollbarSlider.hoverBackground": "#5e797966",
		"scrollbarSlider.activeBackground": "#5e797988",
		"editorGroupHeader.border": "#293333",
		"editorGroup.border": "#3c4344",
		"tab.activeForeground": "#7cc7bf",		
		"tab.inactiveForeground": "#6db1a9aa",		
		"tab.border": "#6db1a922",	
	},
	
	"semanticHighlighting": true,
	
	"semanticTokenColors": {
		"property" : "#B3E0D1",
		"variable.defaultLibrary": "#6DB1A9",
		// "variable.defaultLibrary": "#9b8d8d",
		
		"parameter": { 
			"foreground": "#B3E0D1", 
			"italic": false 
		},			
		"parameter.declaration": { 
			"foreground": "#B3E0D1", 
			"italic": true 
		},	
	},
	
	"tokenColors": [
		{
			"name": "LIGHT – Default color",
			"settings": {
				"foreground": "#eee",
			}
		},
		{
			"name": "COTTON – Base color",
			"scope": [
				"variable",
				"meta.template.expression.js",
				"string.js.taggedTemplate meta.embedded.block.html",
				"string.regexp.js",
				"string.regexp constant.other.character-class.set.regexp",
				"string.regexp.js constant.other.character-class.set.regexp constant.other.character-class.regexp",
				"source.python meta.fstring.python",
				"meta.function-call.arguments.python",
			],
			"settings": {
				"foreground": "#dddddd",
			}
		},	
		{
			"name": "GHOSTLY – Comment tags",
			"scope": [
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#4c6565BB"
			}
		},
		{
			"name": "GHOST – Comments",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#4c6565"
			}
		},	
		{
			"name": "CLOUD – Storage", /* (let, const, function, class); CSS pseudo */
			"scope": [
				"storage",
				"entity.other.attribute-name.pseudo-class.css",
			],
			"settings": {
				"foreground": "#6e8e89"
			}
		},
		{
			"name": "MIST – Punctuation", /* (CLOUD faded) */
			"scope": [
				"meta.brace",
				"punctuation",	
				"entity.other.attribute-name.id.css punctuation.definition.entity.css",
				"entity.other.attribute-name.class.css punctuation.definition.entity.css",
				"constant.character.format.placeholder.other.python",
			],
			"settings": {
				"foreground": "#6e8e89CC"
			}
		},
		{
			"name": "RAIN – Elements", /* punct. accessor + CSS ids and classes" */
			"scope": [
				"entity.name.tag",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"punctuation.accessor",
				"punctuation.separator.key-value.js",
				"keyword.operator.type.annotation.ts",
				"source.css",
			],
			"settings":{
				"foreground": "#7da9a5",
			}
		},
		{
			"name": "SEA – Primitives",
			"scope": [
				"support.variable.dom",
				"support.constant",
				"variable.language",
				"support.class",
				"support.type.primitive.ts",
			],
			"settings": {
				"foreground": "#6db1a9"
			}
		},
		{
			"name": "DEEPSKY – Operators, Math signs",
			"scope": [
				"keyword.operator",
				"keyword.control.anchor.regexp",
			],
			"settings": {
				"foreground": "#82cec5"
			}
		},
		{
			"name": "SKY – JS functions",
			"scope": [
				"entity.name.function",
				"entity.name.function.tagged-template",
				"meta.function-call",
				"source.css support.function",
				"string.js.taggedTemplate punctuation.definition.string.template" // html``
			],
			"settings": {
				"foreground": "#95dcd4",
			}
		},
		{
			"name": "ICE – Obj. keys, Func. params, Attr. names, Props",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key string",
				"entity.other.attribute-name",
				"keyword.operator.quantifier.regexp",
				"support.type.property-name",
				"meta.function-call.arguments.pythonz",
				"support.variable.property",
				"meta.attribute.python",
				"variable.parameter",
				"variable.other.property.js", // this._prop
			],
			"settings": {
				"foreground": "#b3e0d1",
			}
		},
		{
			"name": "FOREST – JS 'new'; CSS units",
			"scope": [
				"keyword.operator.new",
				"keyword.other.unit", // css units
				"entity.other.attribute-name.pseudo-element.css punctuation.definition.entity.css", // ::
			],
			"settings": {
				"foreground": "#83aa8a",
			}
		},		
		{
			"name": "MINT – JS class, @jsdocs",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"entity.other.attribute-name.pseudo-element.css",
			],
			"settings": {
				"foreground": "#aadeb3"
			}
		},
		{
			"name": "SPROUT – All values (string, constant)",
			"scope": [
				"string",
				"constant",
				"source.css support.constant",
			],
			"settings": {
				"foreground": "#90D794",
			}
		},
		{
			"name": "HALO | ITALIC – JS Str template, RegExp; CSS vars, @media",
			"scope": [
				"string.template",
				"string.regexp",
				"variable.css",
				"variable.scss",
				"constant.other.character-class.range.regexp",
				"punctuation.definition.keyword",
				"source.css keyword.control",
				"entity.other.keyframe-offset.percentage.css",
			],
			"settings": {
				"foreground": "#c6eab6", 
				"fontStyle": "italic"
			}
		},
		{
			"name": "HALO faded – JS string template -> faded quotes",
			"scope": [
				"string.template.js punctuation.definition.string.template",
			],
			"settings": {
				"foreground": "#c6eab6AA",
				"fontStyle": ""
			}
		},
		{
			"name": "STONE – JS control flow",
			"scope": [
				"keyword.control",
				"entity.name.label",
				"storage.modifier.async.js",
				"keyword.generator.asterisk.js",
			],
			"settings": {
				"foreground": "#7d8281",
			}
		},
		{
			"name": "PLUM – CSS !important",
			"scope": [
				"keyword.other.important.css",
			],
			"settings": {
				"foreground": "#a290a5"
			}
		},	
		{
			"name": "ROSE – JS debugger, delete, void, regex negation (^)",
			"scope": [
				"keyword.other.debugger",
				"keyword.operator.negation",
				"keyword.operator.expression.delete",
				"keyword.operator.expression.void.js"
			],
			"settings": {
				"foreground": "#ceb0d3"
			}
		},
		{
			"name": "ITALIC – Strings, string templates, func. parameter, :pseudo-class",
			"scope": [
				"string",
				"string.template",
				"variable.parameter",
				"entity.other.attribute-name.pseudo-class.css",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "NOT ITALIC – TAGGED string templates (lit-html)",
			"scope": [
				"string.js.taggedTemplate",
			],
			"settings": {
				"fontStyle": ""
			}
		}
	]
}